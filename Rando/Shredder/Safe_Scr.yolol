S="\nZone:" if :Safezone then S+="Yes" else S+="No" end
S+="\nShip:" if NOT :ShipDamage then S+="Yes" else S+="No" end
S+="\nPlay:" if NOT :PlayDamage then S+="Yes" else S+="No" end
S+="\nColl:" if NOT :CollDamage then S+="Yes" else S+="No" end
S+="\nStat:" if :StationAllowed then S+="Yes" else S+="No" end
:Safe=S goto 1

















